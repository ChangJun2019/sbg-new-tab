{
  "name": "start-new-tab",
  "type": "module",
  "version": "0.0.2",
  "packageManager": "pnpm@8.8.0",
  "description": "A clean, simple, new tab browser extension that supports local image backgrounds",
  "author": {
    "name": "Chang Jun",
    "email": "52chinaweb@gmail.com",
    "url": "https://52chinaweb.com/"
  },
  "license": "MIT",
  "homepage": "https://github.com/ChangJun2019/sbg-new-tab",
  "keywords": [
    "newtab",
    "chrome",
    "extension"
  ],
  "scripts": {
